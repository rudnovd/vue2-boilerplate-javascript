{
  "name": "vue2-boilerplate-javascript",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build --modern --report",
    "lint": "vue-cli-service lint",
